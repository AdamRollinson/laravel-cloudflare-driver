{
    "name": "adamrollinson/laravel-cloudflare-driver",
    "description": "Laravel integration for Cloudflare Workers services",
    "keywords": [
        "Adam Rollinson",
        "laravel",
        "laravel-cloudflare-driver",
        "laravel",
        "php",
        "database",
        "query",
        "sqlite",
        "cloudflare",
        "d1",
        "kv",
        "r2",
        "workers",
        "serverless",
        "serverless functions",
        "serverless framework",
        "serverless architecture"
    ],
    "homepage": "https://github.com/adamrollinson/laravel-cloudflare-driver",
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Rollinson",
            "email": "adamleerollinson@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "doctrine/dbal": "^4.2",
        "illuminate/cache": "^10.0||^11.0",
        "illuminate/contracts": "^10.0||^11.0",
        "illuminate/database": "^10.0||^11.0",
        "saloonphp/saloon": "^3.10",
        "spatie/laravel-package-tools": "^1.16",
        "ext-pdo": "*"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "larastan/larastan": "^2.9",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3",
        "spatie/laravel-ray": "^1.35"
    },
    "autoload": {
        "psr-4": {
            "Rollogi\\LaravelCloudflareDriver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rollogi\\LaravelCloudflareDriver\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "analyse": "vendor/bin/phpstan analyse",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rollogi\\LaravelCloudflareDriver\\LaravelCloudflareDriverServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
